<?xml version="1.0" encoding="utf-8"?>

<odoo>

    <record id="report_visit_card" model="ir.actions.report">
        <field name="name">Patient Visit Report</field>
        <field name="model">aspl.visit</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">clinic_management.report_visit</field>
        <field name="report_file">clinic_management.report_visit</field>
        <field name="binding_type">report</field>
    </record>

    <record id="report_chatter_visit_card" model="ir.actions.report">
        <field name="name">Chatter Report</field>
        <field name="model">aspl.visit</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">clinic_management.report_chatter</field>
        <field name="report_file">clinic_management.report_chatter</field>
        <field name="binding_type">report</field>
    </record>

    <record id="report_invoice_card" model="ir.actions.report">
        <field name="name">Patient Invoice</field>
        <field name="model">aspl.invoice</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">clinic_management.report_invoice</field>
        <field name="report_file">clinic_management.report_invoice</field>
        <field name="print_report_name">'Order-%s' % object.name </field>
        <field name="binding_type">report</field>
    </record>

    <record id="report_appointment_card" model="ir.actions.report">
        <field name="name">Appointment</field>
        <field name="model">aspl.appointment</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">clinic_management.report_appointment</field>
        <field name="report_file">clinic_management.report_appointment</field>
        <field name="print_report_name"> 'Appointment Report'</field>
        <field name="binding_type">report</field>
    </record>

    


</odoo>
