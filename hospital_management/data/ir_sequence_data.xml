<odoo>
    <data>
        <record id="seq_appointment" model="ir.sequence">
            <field name="name">Appointment</field>
            <field name="code">patient_appointment</field>
            <field name="prefix">APT/%(year)s/</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
        </record>
        
        <record id="seq_doctor_visit" model="ir.sequence">
            <field name="name">DoctorVisit</field>
            <field name="code">doctor_visit</field>
            <field name="prefix">VISIT/%(month)s/%(year)s/</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
        </record>

    </data>
</odoo>